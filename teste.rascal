program teste;
var 
   x, y: integer;
   b: boolean;

procedure soma(a: integer; b: integer);
var result: integer;
begin
   result := a + b;
   write(result)
end;

begin
   x := 10;
   y := 20;
   if x < y then
      soma(x, y)
   else
      x := x - 1;
   
   while x > 0 do
   begin
      write(x);
      x := x - 1
   end
end.